{
  "name": "Quicky Calculator",
  "version": "1.0",
  "description": "A calculator extension that allows you to perform calculations on any webpage",
  "manifest_version": 3,
  "icons": {
    "16": "./images/16.png",
    "48": "./images/48.png",
    "128": "./images/128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["style.css"],
    "js": ["main.js"]
  }],
  "action": {
    "default_popup": "index.html"
  }
}
